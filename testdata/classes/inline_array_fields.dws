// Test: Inline Array Types in Class Fields

type TBoard = class
  // Multi-dimensional dynamic array
  Pix : array of array of Integer;

  // Simple dynamic array
  Names : array of String;

  // Static array with range
  Grid : array[1..10] of Integer;

  constructor Create;
end;

constructor TBoard.Create;
begin
  PrintLn('TBoard created');
end;

var board := new TBoard();

