var arr: array of Integer;

// Test .Count property (alias for .Length)
PrintLn('Empty array count: ' + IntToStr(arr.Count));
PrintLn('Empty array length: ' + IntToStr(arr.Length));

// Add some elements
arr.Add(10);
arr.Add(20);
arr.Add(30);
arr.Add(40);

PrintLn('After adding 4 elements, count: ' + IntToStr(arr.Count));
PrintLn('After adding 4 elements, length: ' + IntToStr(arr.Length));

// Test .IndexOf()
PrintLn('IndexOf(20): ' + IntToStr(arr.IndexOf(20)));
PrintLn('IndexOf(30): ' + IntToStr(arr.IndexOf(30)));
PrintLn('IndexOf(99): ' + IntToStr(arr.IndexOf(99))); // Not found, should return -1

// Test .Delete()
arr.Delete(1); // Delete element at index 1 (which is 20)
PrintLn('After Delete(1), count: ' + IntToStr(arr.Count));
PrintLn('After Delete(1), elements: ');
var i: Integer;
for i := 0 to arr.High do
  PrintLn('  [' + IntToStr(i) + '] = ' + IntToStr(arr[i]));

// Test .Delete(index, count)
arr.Add(50);
arr.Add(60);
PrintLn('Added 50 and 60, count: ' + IntToStr(arr.Count));
arr.Delete(1, 2); // Delete 2 elements starting at index 1
PrintLn('After Delete(1, 2), count: ' + IntToStr(arr.Count));
PrintLn('After Delete(1, 2), elements: ');
for i := 0 to arr.High do
  PrintLn('  [' + IntToStr(i) + '] = ' + IntToStr(arr[i]));

PrintLn('done');
